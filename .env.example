# 공공기관 도로명주소 API 키
GOV_JUSO_API_KEY=여기에_발급받은_키

# 네이버 지도 JavaScript API 클라이언트 ID
NEXT_PUBLIC_NAVER_MAP_CLIENT_ID=여기에_네이버지도_클라이언트ID

# PostgreSQL 연결 문자열 (예: Supabase/Neon/RDS)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DBNAME?schema=public

# 데모 모드에서만 인증코드 응답에 노출 (운영은 false 권장)
ALLOW_DEMO_CODE=true

# 결제 모드: mock 또는 live
PAYMENT_MODE=mock
